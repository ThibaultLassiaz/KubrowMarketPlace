<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {# {% block body %}
        <div class="col-lg-12 row">
            {% for possibleKubrow in possibleKubrows %}
                <div class="col-lg-3">
                        <ul>
                            <li><b>Build</b> : {{ possibleKubrow.build }}</li>
                            <li><b>Energy</b> : {{ possibleKubrow.energy }}</li>
                            <li><b>Primary color</b> : {{ possibleKubrow.primary }}</li>
                            <li><b>Secondary color</b> : {{ possibleKubrow.secondary }}</li>
                            <li><b>Tertiary color</b> : {{ possibleKubrow.tertiary }}</li>
                        </ul>
                </div>
            {% endfor %}
        </div>
        {% endblock %} #}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
